{
    "name": "pod_central",
    "version": "3.0.0",
    "description": "A collaboration hub for podcast creators and consumers",
    "main": "server/server.js",
    "homepage": "https://podcentral.herokuapp.com/",
    "scripts": {
        "start": "node server/server.js",
        "dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
        "install": "cd server && npm i && cd ../client && npm i",
        "seed": "cd server && npm run seed",
        "build": "cd client && npm run build"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^5.1.0"
    },
    "dependencies": {
        "@emotion/react": "11.11.0",
        "@emotion/styled": "11.11.0",
        "@mui/material": "5.13.0",
        "axios": "^1.4.0",
        "@cloudinary/react": "1.11.2",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "mongoose": "^7.1.1",
        "podcast-api": "^2.0.3",
        "express": "^4.18.2"
    }
}
